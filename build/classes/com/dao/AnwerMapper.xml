<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace的名字需要跟接口的类名一致 -->
<mapper namespace="com.dao.AnswerMapper">
	<insert id="addAns" parameterType="Answer" >
		insert into answers(anscontent,ansdate,qid) values
		(#{ansContent},#{ansDate},#{qid})
	</insert>
	
	<!-- 查询 -->
	<select id="" >
		
	</select>
	
</mapper>















